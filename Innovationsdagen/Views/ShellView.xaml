<Window x:Class="Innovationsdagen.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Innovationsdagen.Views"
        mc:Ignorable="d"
        Title="ShellView" Height="450" Width="800">
    <Grid>

        
        
        <ListView SelectionMode="Single" Margin="5" ItemsSource="{Binding Servers}">
            <ListView>
                <GridViewColumn Width="60" Header="Name" DisplayMemberBinding="{Binding Name}"/>
                
            </ListView>


        </ListView>
    </Grid>
</Window>
